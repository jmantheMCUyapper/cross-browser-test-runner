{
  "browsers": ["chrome", "firefox", "edge"],
  "test_files": [
    "tests/sample_tests/test_login.py",
    "tests/sample_tests/test_search.py",
    "tests/sample_tests/test_navigation.py"
  ],
  "parallel_execution": false,
  "retry_failed_tests": false,
  "retry_count": 2,
  "screenshot_on_failure": true,
  "timeout": 30,
  "headless": false,
  "window_size": {
    "width": 1920,
    "height": 1080
  },
  "implicit_wait": 10,
  "report_settings": {
    "generate_html": true,
    "generate_json": true,
    "include_screenshots": true
  }
}